<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="App (with MySQL)" type="SpringBootApplicationConfigurationType"
                   factoryName="Spring Boot">
        <module name="yalsee"/>
        <option name="SPRING_BOOT_MAIN_CLASS" value="io.kyberorg.yalsee.Application"/>
        <option name="ENABLE_JMX_AGENT" value="false"/>
        <option name="ACTIVE_PROFILES" value="local"/>
        <option name="ALTERNATIVE_JRE_PATH"/>
        <option name="UPDATE_ACTION_UPDATE_POLICY" value="UpdateClassesAndResources"/>
        <envs>
            <env name="LOG_LEVEL_APP" value="debug"/>
        </envs>
        <method v="2">
            <option name="Make" enabled="true"/>
            <option name="RunConfigurationTask" enabled="true" run_configuration_name="yalsee [clean, package]"
                    run_configuration_type="MavenRunConfiguration"/>
        </method>
    </configuration>
</component>